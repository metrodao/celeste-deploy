{
  "name": "@1hive/celeste-deploy",
  "version": "1.2.0",
  "description": "Aragon Court deployment scripts",
  "author": "Aragon Association",
  "license": "GPL-3.0",
  "scripts": {
    "test": "./node_modules/@aragon/test-helpers/ganache-cli.sh",
    "deploy:court:rpc": "rm -f data/output/minime.rpc.json && rm -f data/output/court.rpc.json && npm run deploy:dai -- -n rpc && npm run deploy:hny -- -n rpc && npm run deploy:court -- -n rpc",
    "deploy:court": "node ./bin/index.js deploy-court --verbose",
    "deploy:dai": "node ./bin/index.js deploy-minime -i ./test/data/input/dai.js --verbose",
    "deploy:hny": "node ./bin/index.js deploy-minime -i data/input/hny.js --verbose",
    "deploy:faucet": "node ./bin/index.js deploy-faucet --verbose",
    "mint": "node ./bin/index.js mint --verbose --network arbtest --amount 30000",
    "upgrade:court": "node ./bin/index.js upgrade-court --verbose",
    "delay:court": "node ./bin/index.js delay-court --verbose",
    "migrate:registry": "node ./bin/index.js migrate-registry --verbose",
    "set:config": "node ./bin/index.js set-config --verbose",
    "set:modules": "node ./bin/index.js set-modules --verbose",
    "set:subscriptions-params": "node ./bin/index.js set-subscriptions-params --verbose"
  },
  "dependencies": {
    "@1hive/celeste": "^0.1.7",
    "@aragon/apps-agent": "^2.0.0",
    "@aragon/apps-token-manager": "^2.1.0",
    "@aragon/apps-voting": "^2.1.0",
    "@aragon/court": "1.1.2",
    "@aragon/court-presale-activate": "1.3.0",
    "@aragon/minime": "^1.0.0",
    "@aragon/templates-externally-owned-presale-bonding-curve": "1.1.0",
    "@aragon/templates-shared": "^1.0.0",
    "@aragon/truffle-config-v5": "^1.0.0",
    "@aragonone/erc20-faucet": "https://github.com/aragonone/erc20-faucet",
    "@aragonone/court-registry-migrator": "^1.0.0",
    "@truffle/config": "1.2.6",
    "@truffle/contract": "4.0.32",
    "@truffle/hdwallet-provider": "^1.4.0",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "truffle-flattener": "^1.4.2",
    "web3": "^1.2.1",
    "web3-eth-abi": "^1.2.4",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@aragon/test-helpers": "^2.1.0",
    "ganache-cli": "^6.4.5",
    "truffle": "5.0.44",
    "arb-ethers-web3-bridge": "^0.7.3"
  }
}
